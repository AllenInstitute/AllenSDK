export PGPASSFILE=/home/docker/.pgpass
export VERBOSE=true
rvm use 1.9.3
sleep 60
echo '*:*:*:postgres:postgres' > $PGPASSFILE
chmod 0600 /home/docker/.pgpass
psql -h db -U postgres lims2docker -c "CREATE USER lims2writer;"
psql -h db -U postgres lims2docker -c "CREATE ROLE lims2writers;"
cd /home/docker/lims
bundle install
rake test:test_load
rake test

