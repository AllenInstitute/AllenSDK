    {
      "_datatype" : "str", "_include" : "optional",
      "_description" : "Narrative description about surgery/surgeries, including date(s) and who performed surgery"

    },
    "protocol" :
    {
      "_datatype" : "str", "_include" : "optional",
      "_description" : "Experimental protocol, if applicable (e.g., IACUC)"

